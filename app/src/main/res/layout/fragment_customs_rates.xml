<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context="com.shipwebsource.mywebsource.MainActivity">

    <TextView
        android:id="@+id/textview_customsRates"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Customs &amp; Excise rates"
        android:textColor="@color/colorPrimary"
        android:textSize="16sp"
        android:fontFamily="sans-serif-medium"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:textAllCaps="true"
        android:layout_marginBottom="10dp"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="17sp"
        android:maxWidth="300dp"
        android:padding="20dp"
        android:layout_below="@id/textview_customsRates"
        android:layout_marginTop="15sp"
        android:text="Expecting something? These are the duty and VAT rates per item category as specified by the laws of the Republic of Trinidad &amp; Tobago"/>



</RelativeLayout>
